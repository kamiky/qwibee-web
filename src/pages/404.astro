---
// This file redirects to the appropriate 404 page based on the mode
// The middleware will handle the routing to /404/app or /404/creator
const creatorMode = Astro.cookies.get("qwb-creator-mode")?.value === "true";

if (creatorMode) {
  return Astro.rewrite("/404/creator");
} else {
  return Astro.rewrite("/404/app");
}
